   <string notr="true">QWidget {
	font-family: &quot;Noto Sans&quot;, &quot;FreeSans&quot;;
	background: #1A1A1A;
	color: whitesmoke;
}

/* QMenuBar and stuff */

QMenuBar, QMenuBar::item {
	background: #1A1A1A;
}
QMenuBar::item:selected {
	background: #008098;
}

QMenu {
	background: #353535;
    border: 1px solid #5A5A5A;
}
QMenu::item:selected {
	background: #008098;
 }
QMenu::item:checked {
	/* distinguish check state for menu item's with images */
}
QMenu::item::disabled {
	color: gray;
}
QMenu::item::disabled:selected {
	color: black;
}
QMenu::separator {
	background: #5A5A5A;
	height: 1px;
	margin: 2px 0;
}

/* QSplitter */

QSplitter::handle,
#mpvFrame,
#outputTextEdit,
#inputLineEdit {
	background: black;
}

#outputTextEdit {
	font: 12px &quot;Courier New&quot;, monospace;
	border-top: 2px solid #1A1A1A;
}

#inputLineEdit {
	font: 12px &quot;Courier New&quot;,monospace;
	padding: 2px;
	border-style: solid;
	border-width: 1px;
	border-color: #1A1A1A black;
}
#inputLineEdit:focus {
	border-color: #008098;
}

/* searchBox */

#searchBox {
	font-size: 12px;
	padding: 2px;
	border-style: solid;
	border-width: 2px;
	border-color: black #1A1A1A;
}
#searchBox:focus {
	border-color: #008098;
}

/* time labels */
#durationLabel {
	font-size: 12px;
}
#seperatorLabel,
#remainingLabel {
	color: gray;
	font-size: 12px;
}

/* QListView */

QListView {
	outline: none;
}
QListView::item {
	padding: 2px 0px;
	background: #141414;
	alternate-background-color: #1A1A1A;
}
QListView::item:alternate {
    background: #1A1A1A;
}
QListView::item:selected {
	background: #008098;
}
QListWidget:item:selected:active,
QListWidget:item:selected:!active {
	color: whitesmoke;
}

/* QScrollBar */

QScrollBar:vertical {
	margin-left: 1px;
	border: none;
	width: 5px;
}
QScrollBar::handle:vertical {
	background: #5A5A5A;
	border-radius: 2px;
	min-height: 10px;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	border: none;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: transparent;
}

QScrollBar:horizontal {
	margin-bottom: 1px;
	border: none;
	height: 5px;
}
QScrollBar::handle:horizontal {
	background: #5A5A5A;
	border-radius: 2px;
	min-width: 10px;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
	border: none;
}
QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal,
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: transparent;
}</string>
